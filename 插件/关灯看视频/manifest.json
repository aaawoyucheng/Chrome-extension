{
    "name": "关灯看视频",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "activeTab",
        "tabs",
        "*://*/*",
        "pageCapture",
        "downloads"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "icons": {
        "16": "icon.png",
        "32": "icon.png",
        "128": "icon.png"
    },
    "page_action": {
        "default_icon": "icon.png"
    },
    "options_page":"option.html"
}
