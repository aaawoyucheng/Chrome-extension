{
   "background": {
      "scripts": [ "ajax_background.js" ]
   },
   "browser_action": {
      "default_icon": "icon48.png"
   },
   "content_scripts": [ {
      "js": [ "jquery-1.10.2.js", "ajax_content.js" ],
      "matches": [ "http://*/*", "https://*/*" ]
   } ],
   "description": "Capture AJAX requests done in browser.  Generate JQuery code to replay such requests.  Execute AJAX and see results.",
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "48": "icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "JQuery AJAX Capture",
   "permissions": [ "webRequest", "webRequestBlocking", "cookies", "tabs", "http://*/*", "https://*/*" ],
   "short_name": "AJAX Capture",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0"
}
