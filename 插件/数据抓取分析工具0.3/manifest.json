{
   "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
   },
   "commands": {
      "toggle-feature-foo": {
         "description": "Toggle feature foo",
         "suggested_key": {
            "default": "Ctrl+Shift+Y",
            "mac": "Command+Shift+Y"
         }
      }
   },
   "content_scripts": [ {
      "css": [ "css/sh-style.css" ],
      "js": [ "js/jquery.js", "js/sh-main.js", "js/sh-implementation.js" ],
      "matches": [ "\u003Call_urls>" ]
   } ],
   "description": "自动生成最简HTML选择命令，用于爬虫网页内容分析",
   "icons": {
      "128": "icon.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "数据抓取分析工具",
   "permissions": [ "activeTab", "tabs", "commands", "https://*/*", "webNavigation" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.3",
   "web_accessible_resources": [ "icon.png", "iframe.html" ]
}
